<%= form_with model: @user, id: "new-user-form" do |f| %>
  <%= f.text_field :name %>
  <%= f.text_field :street_address %>
  <%= f.text_field :city %>
  <%= f.text_field :state %>
  <%= f.number_field :zip_code %>
  <%= f.text_field :email %>
  <%= f.text_field :password %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit "Create" %>
<% end %>
